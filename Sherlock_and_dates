

#include <stdio.h>

int main()
{
	int num;
	scanf("%d", &num);
	for(int i=0;i<num;i++)
	{
		int dd,yyyy;
		char mm[10];
		scanf("%d %s %d",&dd,mm,&yyyy);
		if(dd!=1)
		{
			printf("%d %s %d",dd-1,mm,yyyy);
		}
		else
		{
			if(mm[0]=='J'&&mm[1]=='a')
			{
				dd=31;
				yyyy=yyyy-1;
				printf("%d",dd);
				printf(" December %d",yyyy);
			}
			else if(mm[0]=='F')
			{
				dd=31;
				printf("%d",dd);
				printf(" January %d",yyyy);
			}
			else if(mm[0]=='A'&&mm[1]=='p')
			{
				dd=31;
				printf("%d",dd);
				printf(" March %d",yyyy);
			}
			else if(mm[0]=='M'&&mm[1]=='a'&&mm[2]=='y')
			{
				dd=30;
				printf("%d",dd);
				printf(" April %d",yyyy);
			}
			else if(mm[0]=='J'&&mm[1]=='u'&&mm[2]=='n')
			{
				dd=31;
				printf("%d",dd);
				printf(" May %d",yyyy);
			}
			else if(mm[0]=='J'&&mm[1]=='u'&&mm[2]=='l')
			{
				dd=30;
				printf("%d",dd);
				printf(" June %d",yyyy);
			}
			else if(mm[0]=='A'&&mm[1]=='u')
			{
				dd=31;
				printf("%d",dd);
				printf(" July %d",yyyy);
			}
			else if(mm[0]=='S')
			{
				dd=31;
				printf("%d",dd);
				printf(" August %d",yyyy);
			}
			else if(mm[0]=='O')
			{
				dd=30;
				printf("%d",dd);
				printf(" September %d",yyyy);
			}
			else if(mm[0]=='N')
			{
				dd=31;
				printf("%d",dd);
				printf(" October %d",yyyy);
			}
			else if(mm[0]=='D')
			{
				dd=30;
				printf("%d",dd);
				printf(" November %d",yyyy);
			}
			else if(mm[0]=='M'&&mm[1]=='a'&&mm[2]=='r')
			{
				if((yyyy%400==0)||((yyyy%4==0)&&(yyyy%100!=0)))
				{
					dd=29;
					printf("\n%d",dd);
					printf(" February %d",yyyy);
				}
				else
				{
					dd=28;
					printf("\n%d",dd);
					printf(" February %d",yyyy);
				}
			}

		}
		printf("\n");
	}
}
